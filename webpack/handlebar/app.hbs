<!DOCTYPE HTML>
<html lang="en">

<head>
    {{> components/documentHead }}


    {{#if customPrimaryColour}}

    {{{cssvar
    "--primary-text-colour" customPrimaryColour
    "--primary-text-colour2" customPrimaryColour
    }}}

    {{/if}}

    {{!-- JSON-LD --}}
    <script type="application/ld+json">
        {
            "@context": "https://schema.org/",
            "@type": "SoftwareApplication",
            "name": "{{assApp.appName}}",
            "url": "{{assApp.link}}",
            "about": "{{assApp.description}}",
            "description": "{{assApp.description}}",
            "image": "{{assApp.image}}",
            "applicationCategory": "Application",
            "author": {
                "@type": "Person",
                "name": "{{kurt.name}}",
                "givenName": "{{kurt.name}}",
                "familyName": "{{kurt.name}}",
                "additionalName": "{{kurt.additionalName}}",
                "url": "{{kurt.website}}",
                "image": "{{kurt.image}}",
                "description": "{{meta.description}}",
                "jobTitle": "Software Engineer"
            },
            "creator": {
                "@type": "Person",
                "name": "{{kurt.name}}",
                "givenName": "{{kurt.name}}",
                "familyName": "{{kurt.name}}",
                "additionalName": "{{kurt.additionalName}}",
                "url": "{{kurt.website}}",
                "image": "{{kurt.image}}",
                "description": "{{meta.description}}",
                "jobTitle": "Software Engineer"
            },
            "offers": {
                "@type": "offer",
                "price": "0",
                "priceCurrency": "USD",
                "name": "{{assApp.appName}}",
                "url": "{{assApp.link}}",
                "image": "{{assApp.image}}",
                "description": "{{assApp.description}}",
            },
            "inLanguage": "en",
            "dateCreated": "{{date}}",
            "dateModified": "{{date}}",
            "datePublished": "{{date}}"
        }
    </script>
</head>

<body class="is-preload app-specific">
    <div id="page-wrapper" class="bootstrap-wrapper">
        <header id="header" class="app-specific alt">
            <a href="/" class="header-logo">
                <img src="{{icon}}" alt="Logo" />
            </a>
            <div class="header-center">
                <a href="/" title="{{meta.title}}">Home</a>
                <a href="{{urlref link.documentation}}" title="Documentation" target="_blank"
                    rel="noopener noreferrer">Documentation</a>
                <a href="{{urlref link.discord}}" title="Discord" target="_blank" rel="noopener noreferrer">Discord</a>
            </div>
            <div class="header-download">
                <a href="{{urlref link.downloadApp}}" title="Download" target="_blank"
                    rel="noopener noreferrer">Download</a>
            </div>
        </header>

        <hr />


        <section id="banner">
            <div class="inner">
                <div class="row">
                    <div class="banner-text col-12 col-xl-6 col-lg-6 col-md-12 col-sm-12 col-xs-12">
                        <p class="primary-text font-weight-heavy">Download</p>
                        <h2 class="font-weight-light">
                            <span class="font-weight-light">{{titleStart}}</span><br />
                            <span class="font-weight-heavy">{{titleEnd}}</span>
                        </h2>
                        <p>{{description}}</p>
                    </div>

                    <div class="banner-screenshot-stack col-12 col-xl-6 col-lg-6 col-md-12 col-sm-12 col-xs-12">
                        <div class="star-parallax">
                            <div data-depth="0.05" class="star-container">
                                <div class="star four-pointed-star"></div>
                            </div>
                            <div data-depth="0.75" class="star-container">
                                <div class="star four-pointed-star"></div>
                            </div>
                            <div data-depth="0.33" class="star-container">
                                <div class="star four-pointed-star"></div>
                            </div>
                            <div data-depth="0.25" class="star-container">
                                <div class="star four-pointed-star"></div>
                            </div>
                            <div data-depth="0.05" class="star-container">
                                <div class="star four-pointed-star"></div>
                            </div>
                        </div>

                        <div class="screenshot-stack">
                            <div class="apps-screenshot">
                                <div class="screenshot-inner">
                                    <div class="apps-left-screenshot-slider-bg"></div>
                                    <div class="assistantapps-logo"></div>
                                    <img src="{{screenshotLeft}}" loading="lazy" draggable="false" alt="screenshotLeft"
                                        class="screenshotLeft" />
                                    <img src="/assets/img/screenshot-frame.png" loading="lazy" draggable="false"
                                        alt="phone frame" class="screenshot-frame" />
                                </div>
                            </div>
                            <div class="apps-screenshot">
                                <div class="screenshot-inner">
                                    <div class="apps-left-screenshot-slider-bg"></div>
                                    <div class="assistantapps-logo"></div>
                                    <img src="{{screenshotRight}}" loading="lazy" draggable="false"
                                        alt="screenshotRight" class="screenshotRight" />
                                    <img src="/assets/img/screenshot-frame.png" loading="lazy" draggable="false"
                                        alt="phone frame" class="screenshot-frame" />
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>

        <hr />

        {{> components/footer }}
    </div>

    {{> components/background }}

    {{> components/scripts parallax="true" }}

</body>

</html>